<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ovopark.tao.mp.mapper.MainMapper">
    <select id="getAll" resultType="com.ovopark.tao.mp.entity.po.Main">
        select * from c_main
     </select>


    <select id="getById" resultType="com.ovopark.tao.mp.entity.po.Main">
